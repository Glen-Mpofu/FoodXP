{
  "expo": {
    "name": "FoodXP",
    "slug": "FoodXP",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "scheme": "foodxp",
    "notification": {
      "icon": "./assets/foodxp/notifications.png",
      "color": "#ffffff",
      "androidMode": "default",
      "androidCollapsedTitle": "FoodXP"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.glenmpofu.FoodXP",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSLocationWhenInUseUsageDescription": "FoodXP needs your location to show nearby restaurants and calculate delivery fees.",
        "NSLocationAlwaysUsageDescription": "FoodXP needs your location to provide delivery services."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.glenmpofu.FoodXP",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "NOTIFICATIONS",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/foodxp/notifications.png",
          "color": "#ffffff",
          "sounds": []
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow FoodXP to use your location to show nearby restaurants and calculate delivery fees.",
          "locationWhenInUsePermission": "Allow FoodXP to use your location to show nearby restaurants and calculate delivery fees."
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d77f92d9-65ad-483f-9cc7-ab004929f3b0"
      }
    }
  }
}